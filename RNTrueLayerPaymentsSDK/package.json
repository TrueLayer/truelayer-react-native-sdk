    "@types/react": "^18.0.25",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
